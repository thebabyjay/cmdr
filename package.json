{
  "name": "cmdr",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "dotenv -e .env -- tauri",
    "notarize": "dotenv -e .env -- sh -c 'xcrun notarytool submit \"$1\" --apple-id \"$APPLE_ID\" --team-id \"$APPLE_TEAM_ID\" --password \"$APPLE_PASSWORD\" --wait --verbose' --",
    "notarize:log": "dotenv -e .env -- sh -c 'xcrun notarytool log \"$1\" --apple-id \"$APPLE_ID\" --team-id \"$APPLE_TEAM_ID\" --password \"$APPLE_PASSWORD\"' --",
    "notarize:history": "dotenv -e .env -- sh -c 'xcrun notarytool history --apple-id \"$APPLE_ID\" --team-id \"$APPLE_TEAM_ID\" --password \"$APPLE_PASSWORD\"' --"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.49.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "dotenv-cli": "^7.4.2",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
  }
}
